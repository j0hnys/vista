{
    "name": "j0hnys/vista",
    "description": "laravel UI generator using iview, vue",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "John Stamkos",
            "email": "tzonyss@gmail.com"
        }
    ],
    "require": {
        "mustache/mustache": "2.12.0",
        "doctrine/dbal": "2.9.1"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "^3.8"
    },
    "minimum-stability": "alpha",
    "autoload": {
        "psr-4": {
            "j0hnys\\Vista\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "j0hnys\\Vista\\Tests\\": "tests/"
        },
        "classmap": [
            "tests/sandbox"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "j0hnys\\Vista\\VistaServiceProvider"
            ]
        }
    }
}
